{% extends 'base.html.twig' %}

{% block content %}
    <div id="content">
        <img src="{{ asset('images/Subgroup/Rhythm/banner.png') }}">
        <p id="intro">
            Community of gamers with an active interest in rhythm-based games. We accept any and all skill levels. Even if you're brand new to the games, we'll help you and teach you.
        </p>
        <div class="community-important">
            <div class="community-important-info-header">Meetings</div>
            <div class="community-important-info">SAU Cafeteria: Wednesdays 8PM-11PM</div>
            <div class="community-important-info">Gol 3445: Sundays 2PM-5PM</div>
        </div>
        <br>
        <div class="board-header">E-Board</div>
        {% include 'utility/board.html.twig' %}
    </div>
{% endblock %}

{% block stylesheets %}
    {{  parent() }}
    <link rel="stylesheet" type="text/css" href="{{ asset('css/websiteStyles2.css') }}">
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('js/homepage/homepage.js') }}"></script>
{% endblock %}
